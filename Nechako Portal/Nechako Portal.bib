@article{place_kill_2011,
	title = {Kill the lake? kill the proposal: accommodating {First} {Nations}’ environmental values as a first step on the road to wellness},
	volume = {76},
	issn = {0343-2521, 1572-9893},
	shorttitle = {Kill the lake?},
	url = {http://link.springer.com/article/10.1007/s10708-009-9286-5},
	doi = {10.1007/s10708-009-9286-5},
	abstract = {In recent years, a buoyant global market for minerals has led to renewed interest in the mining sector as a means of generating economic growth in resource producing areas of developed nations. The development of new operations, however, raises concerns around the impacts of mining activities on the environment, health and traditional ways of life of Indigenous peoples. Using the proposed expansion of the Kemess mine in northern British Columbia as a case study, this paper examines two First Nations’ perspectives regarding the regulatory process through which environmental values may be validated and protected, and seeks to understand how these First Nations’ environmental values and perceptions of risk are connected to health and well-being. Sixteen key informant interviews were carried out and thematically analyzed through political ecology and environmental justice frameworks. The paper argues that accommodating First Nations’ environmental values and perceptions of risks is a necessary first step to reclaim health and well-being in politically marginalized settings.},
	language = {en},
	number = {2},
	urldate = {2017-01-29},
	journal = {GeoJournal},
	author = {Place, Jessica and Hanlon, Neil},
	month = apr,
	year = {2011},
	pages = {163--175},
