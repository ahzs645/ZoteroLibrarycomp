	journal = {Canadian Journal of Fisheries and Aquatic Sciences},
	author = {Hutchings, J A and Walters, C and Haedrich, R L},
	month = may,
	year = {1997},
	pages = {1198--1210},
	file = {NRC Research Press PDF fulltext:/Users/ahmadjalil/Zotero/storage/9VDBV66U/Hutchings et al. - 1997 - Is scientific inquiry incompatible with government.pdf:application/pdf;NRC Research Press Snapshot:/Users/ahmadjalil/Zotero/storage/737MWUKZ/f97-051.html:text/html},
