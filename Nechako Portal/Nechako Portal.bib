@article{depew_overview_2013,
	title = {An overview of mercury concentrations in freshwater fish species: a national fish mercury dataset for {Canada}},
	volume = {70},
	issn = {0706-652X},
	shorttitle = {An overview of mercury concentrations in freshwater fish species},
	url = {https://cdnsciencepub.com/doi/10.1139/cjfas-2012-0338},
	doi = {10.1139/cjfas-2012-0338},
	abstract = {Fish mercury (Hg) concentrations have been measured over the last 30–40 years in all regions of Canada as part of various monitoring and research programs. Despite this large amount of data, only regional assessments of fish Hg trends and patterns have previously been attempted. The objective of this study was to assemble available freshwater fish Hg concentration data from all provinces and territories and identify national patterns. The Canadian Fish Mercury Database includes over 330 000 records representing 104 species of freshwater fish collected from over 5000 locations across Canada between 1967 and 2010. Analysis of the 28 most frequently occurring species ({\textgreater}1000 records) showed that the majority of variation in Hg concentrations (when normalized to a standard size) was accounted for by geographic location. Median Hg concentrations increased with trophic level (r = 0.40, p {\textless} 0.05), with the highest Hg concentrations found in piscivorous species such as walleye (Sander vitreus), northern pike (Esox lucius), and lake trout (Salvelinus namaycush). The Canadian Fish Mercury Database provides the most comprehensive summary of fish Hg measurements in Canada, and the results indicate that several regionally observed trends in fish Hg concentrations (e.g., Hg biomagnification and geographic variation) are observed at a national scale. Implications for the effective assessment of changes in fish Hg concentrations in relation to changes in Hg emission regulations are discussed.},
	language = {English},
	number = {3},
	urldate = {2024-02-01},
	journal = {Canadian Journal of Fisheries and Aquatic Sciences},
	author = {Depew, David C. and Burgess, Neil M. and Anderson, M. Robin and Baker, Randy and Bhavsar, Satyendra P. and Bodaly, R.A. (Drew) and Eckley, Chris S. and Evans, Marlene S. and Gantner, Nikolaus and Graydon, Jennifer A. and Jacobs, Kevin and LeBlanc, Jason E. and St. Louis, Vincent L. and Campbell, Linda M.},
	month = mar,
	year = {2013},
	keywords = {25.a Environment / Ecosystems, 7.a Quantitative, 2.o Lake Trout, 2.o Walleye},
	pages = {436--451},
}

@techreport{gf_gunville_inventory_1976,
	title = {An inventory of the {Driftwood} {River} and its tributaries},
	abstract = {The Driftwood river and its tributaries are very productive as
exemplified by the high quality spawning, rearing and resident habitat which is suitable for all of the fishes native to the Stuart drainage including sockeye. Lion and Kastburg creeks have exceptional potential for spawning and rearing below and above the barriers. Fish ladders on these streams would undoubtably be a worthwhile endeavour opening up many miles of first class habitat and consequently increasing salmonid production. Resident habitat appears good throughout the system, holding
areas, cover and pools are ubiquitous. Dolly Varden in the vicinity of 15-20 lbs are widely rumoured. Although we did not actually catch anyof that size we did see one or two while floating down the river which would approach 12 lbs.},
	language = {English},
	institution = {Fish and Wildlife Branch - Victoria B.C.},
	author = {{G.F. Gunville}},
