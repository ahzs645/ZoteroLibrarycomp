	author = {McAdam, Steven O.},
	month = may,
	year = {2011},
	pages = {812--822},
	file = {NRC Research Press PDF fulltext:/Users/ahmadjalil/Zotero/storage/GGJ87W3D/McAdam - 2011 - Effects of substrate condition on habitat use and .pdf:application/pdf;NRC Research Press Snapshot:/Users/ahmadjalil/Zotero/storage/4C6VXPKW/f2011-021.html:text/html},
