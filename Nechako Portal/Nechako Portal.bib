@book{mcdonald_long_2021,
	address = {Vancouver},
	series = {The {C}.{D}. {Howe} {Series} in {Canadian} {Political} {History}},
	title = {A {Long} {Way} to {Paradise}: {A} {New} {History} of {British} {Columbia} {Politics}},
