@article{calvert_seismic_2011,
	title = {Seismic reflection constraints on upper crustal structures in the volcanic-covered central {Nechako} basin, {British} {Columbia}},
	volume = {48},
	issn = {00084077},
	url = {http://www.nrcresearchpress.com/doi/abs/10.1139/e10-097#.WBNuncm2Hzg},
	doi = {10.1139/e10-097},
	abstract = {In 2008, a Vibroseis seismic reflection survey was acquired by Geoscience BC across the eastern part of the volcanic-covered Nechako basin in central British Columbia, where Cretaceous sedimentary rocks have been exhumed along a NNW trend. Good signal penetration through the volcanic cover is indicated by lower crustal reflections at 8-12 s, which were recorded by the entire seismic survey. Comparison of the 2008 seismic survey with data from a previous survey indicates that the lack of reflectivity in the earlier surveys is generally representative of the subsurface geology. The seismic data show that ∼1700 and ∼2900 m thick sub-basins are present at the northern and southern ends of this trend, but the intervening Cretaceous rocks are discontinuous and relatively thin. The creation of a passive-roof duplex by Campanian or later low-angle thrusting is inferred within the thickest Cretaceous strata, but elsewhere faulting is likely related to Eocene extension or transtension. Seismic reflections are also recorded from folded volcanic stratigraphy, the base of the surface volcanic rocks, an underlying volcaniclastic stratigraphy, and intrusions projecting into a Quaternary volcanic cone. Seismic interpretation is complemented by coincident audiofrequency magnetotelluric surveys, from which faulting is inferred at offsets in a regional conductor. No regionally extensive stratigraphy can be identified within the seismic data, and the central Nechako basin appears to be a complex network of small, deformed sub-basins, rather than a single large basin. (English)},
	number = {6},
	urldate = {2014-04-22},
	journal = {Canadian Journal of Earth Sciences},
	author = {Calvert, A.j. and Hayward, N.e. and Spratt, J.e. and Craven, J.a. and Spence, George},
	year = {2011},
	keywords = {British Columbia, EARTH sciences, NECHAKO River Watershed (B.C.), MAGNETOTELLURIC prospecting, SEISMIC reflection method, VIBROSEIS, VOLCANOLOGY},
	pages = {1021--1037},
	file = {EBSCO Full Text:/Users/ahmadjalil/Zotero/storage/PGQDSXG9/Calvert et al. - 2011 - Seismic reflection constraints on upper crustal st.pdf:application/pdf},
}

