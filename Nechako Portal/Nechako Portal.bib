	journal = {Global Mental Health},
	author = {Pearce, M. E. and Jongbloed, K. A. and Pooyak, S. D. and Blair, A. H. and Christian, W. M. and Sharma, R. and Mazzuca, A. and Zamar, D. S. and Schechter, M. T. and Spittal, P. M.},
