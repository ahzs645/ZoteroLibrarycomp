	journal = {Alternatives Journal},
	author = {Christensen, Bev},
	month = oct,
	year = {1996},
	keywords = {Environmental aspects, Saturation search (2), Social Sciences: Comprehensive Works, Environmental Studies, Book reviews, Hydroelectric plants},
	pages = {30--31},
}

@article{christie_indigenous_2013-1,
