	author = {Ambus, Lisa and Davis-Case, D'Arcy and Tyler, Stephen},
	month = sep,
	year = {2007},
	keywords = {British Columbia, community forestry, small tenure, woodlot},
	file = {Full Text PDF:/Users/ahmadjalil/Zotero/storage/9VZVFDFN/Ambus et al. - 2007 - Big expectations for small forest tenures in Briti.pdf:application/pdf;Snapshot:/Users/ahmadjalil/Zotero/storage/2B29RD3B/512.html:text/html},
}

