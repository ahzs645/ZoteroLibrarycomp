@article{gibson_heritage_1995,
	title = {Heritage impact assessment and management in {British} {Columbia}},
	volume = {23},
	copyright = {Copyright University of Waterloo. Faculty of Environmental Studies 1995},
	issn = {07116780},
	url = {http://search.proquest.com/docview/207673832/abstract/F5F28C7905004EF5PQ/1},
	abstract = {The purpose of the Heritage Conservation Act was to encourage and facilitate the conservation of British Columbia heritage resources. The resources were limited to four classes-historical, architectural, paleological and scenic. The statute created an appointed Board to advise the government and an agency, the Heritage Conservation Branch, to manage heritage resources in the province. The Branch attempted to bring together and integrate into a single heritage management team, a diverse group of professionals that included art historians, historians, architects, and physical planners. But in the first statute, there was no mention of heritage impact assessment. Following an internal Branch report, Guidelines for Heritage Resource Impact Assessment in British Columbia [Germann, 1981], the reference to impact assessment was added in 1981 when the Heritage Conservation Act was amended, [British Columbia, Legislature 1981]. The amendment, however, limited the application of heritage impact assessment to cases outside municipalities.
The first important example of a formal heritage impact assessment in British Columbia was associated with the proposal of the British Columbia Hydro Power Corporation to dam the Peace River at Site "C" and flood 6000 hectares that contained 250 recorded pre-contact and historic heritage sites [British Columbia, Ministry of Municipal Affairs, Recreation and Culture, 1989]. In 1981 the Branch had supervised a heritage impact assessment that made recommendations to the Utilities Commission concerning mitigation and compensation for the unavoidable loss of heritage resources. But the Commission which convened public hearings as a formal component of the review process rejected the \$5,965,000 mitigation and compensation package that had been recommended. The Commission was critical of both the research and its high costs, as well as the reasoning behind the recommendations. Due to issues unrelated to the mitigation recommendations, plans for the dam were put aside. Site "C" provides an unhappy example of heritage impact assessment since it appeared to have little or no influence on project planning. Indeed, prior to 1985 there are few, if any, good examples of heritage impact assessments which influenced development prospects in a significant way.
After 1987, developers of large projects requiring EIA reviews had to face also the requirement of mandatory heritage impact assessment [[R. Kenny], 1991], along with social, economic and biophysical impact assessment. Until 1987 the only review guide with written regulatory reference to heritage impact assessment had been the Guide to the Energy Project Review Process, 1982 [British Columbia, Ministry of Energy Mines and Petroleum Resources, 1982]. Things changed after 1987 when many guides were reorganized, simplified, streamlined and made more explicit regarding regulatory heritage assessments. The Major Project Review Guidelines 1990 [British Columbia, Ministry of Regional and Economic Development and British Columbia Environment, 1990]; the Mines Development Review Process, [British Columbia, Ministry of Energy Mines and Petroleum Resources, 1989]; and the planned forest licence review guidelines all now contain explicit regulations regarding heritage impact assessment [O'Gorman, 1991].},
	language = {English},
	number = {1},
	urldate = {2017-01-10},
	journal = {Environments},
	author = {Gibson, Edward M. and Day, J. Chad},
	year = {1995},
	keywords = {Environmental Studies, Environmental policy, Housing And Urban Planning, Environmental impact statements, Historic preservation},
	pages = {10--15},
	file = {Full Text Snapshot:/Users/ahmadjalil/Zotero/storage/EFFF54H9/1.html:text/html},
}

@phdthesis{morgenroth_elastic_2016,
	title = {Elastic stress modelling and prediction of ground class using a {Bayesian} {Belief} {Network} at the {Kemano} tunnels},
	url = {https://open.library.ubc.ca/cIRcle/collections/ubctheses/24/items/1.0308775},
	abstract = {The Kemano hydroelectric facility was constructed in the 1950s to supply power to the
aluminum smelter in Kitimat, on the west coast of British Columbia. The Kemano project
includes a 16 km long water conveyance tunnel that set world record advance rates in the
1950s, and 8 km of a partially completed tunnel.
A risk management strategy was developed in the late 1980s in case of collapse of the first water conveyance tunnel, and by 1990 the excavation of a second tunnel parallel to the first had begun. Work halted in 1991 due to environmental litigation and change in political climate. In 2011 the owner of the Kitimat smelter and the Kemano hydroelectric facility announced plans to continue work on the tunnel that was left unfinished. This thesis is a collaboration with Hatch Ltd., a consultant to the owner, to determine the ground conditions and support requirements that should be anticipated in completing the backup tunnel. Three dimensional finite element elastic stress modelling was completed in order to determine the in-situ stress conditions as well as the boundary stresses around the tunnel. The modelling results were used to estimate where stress-induced problem areas should be expected, for example at chainages 10+700 to 12+700 in the backup tunnel. The results of the stress modelling were incorporated into a Bayesian Belief Network that was developed for the Kemano tunnels. It was built using widely accepted empirical relationships in rock mechanics, expert judgement and conditional relationships between inputs. This network predicts the ground class at a user-defined chainage, based on a database that was developed from project literature. The user is also able to input new data
as it becomes available, for example during the tunnel advance. The predictions from the network align with what can be seen in the excavated portion of the backup tunnel, for example accurately predicting the need for steel sets at chainage 8+510. The predicted ground class was plotted as a function of chainage, and may be used as a comparison to the support requirements that have been determined thus far.},
	language = {eng},
	urldate = {2017-01-10},
	school = {University of British Columbia},
	author = {Morgenroth, Josephine S.},
	month = aug,
	year = {2016},
	file = {Full Text PDF:/Users/ahmadjalil/Zotero/storage/DJHP9HTD/Morgenroth - 2016 - Elastic stress modelling and prediction of ground .pdf:application/pdf;Snapshot:/Users/ahmadjalil/Zotero/storage/98JDBCWU/1.html:text/html},
}

