	number = {8},
	urldate = {2017-01-10},
	journal = {Canadian Journal of Earth Sciences},
	author = {Rouse, G. E. and Mathews, W. H.},
	month = aug,
	year = {1988},
	pages = {1268--1276},
	file = {NRC Research Press PDF fulltext:/Users/ahmadjalil/Zotero/storage/5JSNTEPJ/Rouse and Mathews - 1988 - Palynology and geochronology of Eocene beds from C.pdf:application/pdf;NRC Research Press Snapshot:/Users/ahmadjalil/Zotero/storage/H4RIM2Z7/e88-122.html:text/html},
