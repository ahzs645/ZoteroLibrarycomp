@techreport{j_goodman_francois_1962,
	title = {Francois {Lake} {Kokanee}},
	abstract = {The only known population of spring spawning kokanee in the Fraser watershed is found in Francois Lake. Although this poplation has not been included in the general kokanee investigation, it appears worthwhile to summarize the findings from the limited number of samples collected. Not only is this a most unusual population because of its time of spawning, but it is the only kokanee population sampled which is undoubtedly reared in Francois Lake. 

