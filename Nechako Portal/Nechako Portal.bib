	journal = {Environmental Science \& Technology},
	author = {McCutcheon, Jenine and Power, Ian M. and Harrison, Anna L. and Dipple, Gregory M. and Southam, Gordon},
	month = aug,
	year = {2014},
	keywords = {2.m Water, 25.a Environment / Ecosystems, 7.b Qualitative},
	pages = {9142--9151},
}

