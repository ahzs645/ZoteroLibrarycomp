	number = {9},
	urldate = {2016-06-07},
	journal = {Canadian Journal of Civil Engineering},
	author = {Desreumaux, Quentin and Côté, Pascal and Leconte, Robert},
	month = aug,
	year = {2014},
	pages = {839--844},
	file = {NRC Research Press PDF fulltext:/Users/ahmadjalil/Zotero/storage/VNZAHDKF/Desreumaux et al. - 2014 - Role of hydrologic information in stochastic dynam.pdf:application/pdf;NRC Research Press Snapshot:/Users/ahmadjalil/Zotero/storage/Z5HI47IC/cjce-2013-0370.html:text/html},
