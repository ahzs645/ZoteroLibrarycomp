	volume = {7},
	copyright = {Authors and the owners of copyright in the works will be required to either assign copyright, agree to co-own copyright, or assign a publication license in any works approved for publication by the Journal of Ecosystems and Management.  Please complete the  Copyright Release Form  and return it to the Managing Editor.},
	shorttitle = {Predicting the risk of wet ground areas in the {Vanderhoof} {Forest} {District}},
	url = {http://www.jem-online.org/forrex/index.php/jem/article/view/543},
	abstract = {The mountain pine beetle epidemic is changing British Columbia forests and watersheds at the landscape scale. Watersheds with dead-pine-leading stands in the Vanderhoof Forest District of central British Columbia are reported to have wet soils due to raised water tables. They report a conversion of summer logging ground (dry firm soil) to winter logging ground (wetter less firm soil), upon which forestry equipment operation is difficult or impossible before freeze-up. This paper outlines a project that explores this serious operational issue through the perspective of the hydrologic water balance. It aims to determine the spatial extent of wet ground areas and to provide operational guidance through the development of a model that can predict where wet ground may occur at the stand and watershed level. The watershed-level prediction described here will be based on risk indicators developed from available geographic information system data and aerial photographs, as well as local knowledge. Predictions will be qualified through field verification studies at representative stands within ranked watersheds. Preliminary results are presented.},
	number = {2},
	urldate = {2016-11-23},
	journal = {Journal of Ecosystems and Management},
	author = {Rex, John and Dubé, Stephane},
	month = jul,
	year = {2006},
	keywords = {risk assessment, Mountain Pine Beetle, risk indicators, soil hydrology, water balance},
	file = {Full Text PDF:/Users/ahmadjalil/Zotero/storage/9PIGQ62E/Rex and Dubé - 2006 - Predicting the risk of wet ground areas in the Van.pdf:application/pdf;Snapshot:/Users/ahmadjalil/Zotero/storage/BTMB3FN6/543.html:text/html},
}

