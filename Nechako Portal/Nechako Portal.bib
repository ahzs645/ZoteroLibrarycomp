	journal = {Trials},
	author = {Jongbloed, Kate and Friedman, Anton J. and Pearce, Margo E. and Van Der Kop, Mia L. and Thomas, Vicky and Demerais, Lou and Pooyak, Sherri and Schechter, Martin T. and Lester, Richard T. and Spittal, Patricia M.},
