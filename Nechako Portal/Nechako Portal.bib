@techreport{r_bussanich_fall_2018,
	address = {Westbank, BC},
	title = {Fall hydroacoustic trawl surveys at {Stuart} {Lake} and {Trembleur} {Lake}, {BC}, 2017},
	abstract = {A fall juvenile salmonid hydroacoustic survey was implemented at Stuart and Trembleur Lake in 2017. Hydroacoustic data was collected using a BioSonics DT-X split beam echo sounder with a 200 kHz transducer. The lake surveys replicated previous DFO surveys with our 2017 results outlined in this report.

Fall nerka fry from Stuart and Trembleur Lake were captured using a mid-water trawl to determine their size and age composition. Stuart Lake trawling in 2017 resulted in the capture of 281 nerka fry. Juvenile trawl catches were composed of 87\% age-0, and 2\% age-1 nerka. The remaining catch included sculpin (31), whitefish (3), and burbot (1). Mean length and weight of age-0 fry was 73 mm and 4.4 g, while age-1 fry was 172 mm and 61.8 g, respectively. The highest densities of fish were captured within Region 1 (Westerly area). Trembleur Lake trawling in 2017 resulted in the capture of 479 nerka fry. Juvenile trawl catches were composed of 90\% age-0, and 9\% age-1 nerka. The remaining catch included whitefish (4). Mean length and weight of age-0 fry was 81 mm and 6.0 g, while age-1 fry was 172 mm and 58.1 g, respectively. The highest densities of fish were captured in Region 2 (Central area).

The Stuart Lake hydroacoustic densities were clustered near the westerly area of the lake. Tracked densities of the total fish population were highest from 5 m to 35 m, and mean tracked densities ranged from 130 fish / ha to 662 fish /ha. A limnetic population estimate of 5,140,953 for 2017 was determined from echo counting. Corresponding population estimates for “small” classed nerka of 1,592,987 (47 fish/ha) was determined to be a ‘conservative’ estimate due to unsteady field collection-lake conditions.

The Trembleur Lake hydroacoustic densities were clustered near the southwestern and central area of the lake. Tracked densities of the total fish population were highest from 10 m to 32 m, and mean tracked densities ranged from 178 fish / ha to 1,447 fish /ha. A limnetic population estimate of 5,082,770 for 2017 was determined from echo counting. Corresponding population estimates for “small” classed nerka of 2,103,056 (212 fish/ha) was determined to be ‘robust’ due to steady field collection-lake conditions.

A sensitivity analysis comparing echo counting and echo integration techniques showed consistent covariation of fish targets per hectare for both Stuart and Trembleur Lake. Noise scatter and instability of the tow body from the lake surface state at Stuart Lake rendered our interpretation of data to be of lower quality when compared to the Trembleur data. Three methods of analysis and processing were compared, of which echo counting techniques using 2 m depth bins, horizontally stratified at 250 m intervals, provided a median estimate; thus considered the final population estimator for both lakes in 2017.

Low escapement and low fry recruitment may be limiting nerka production from Stuart and Trembleur lakes. Recommendations for additional monitoring and associated activities are provided.},
	number = {GS18819003A},
	institution = {Okanagan Nation Alliance Fisheries},
	author = {{R. Bussanich} and {C. Mathieu} and {C. Tonasket} and {N. Yaniw} and {D. Degan} and {A-M. Mueller}},
	month = jan,
	year = {2018},
	keywords = {25.a Environment / Ecosystems, 7.a Quantitative, 2.c Indigenous Groups, 2.o  Nerka Salmon},
	pages = {31},
}

@techreport{rs_cope_responses_1998,
	title = {Responses of {Sockeye} {Salmon} ({Oncorhynchus} nerka) {Embryos} to {Intragravel} 	{Incubation} {Environments} in {Selected} {Streams} within the {Stuart}-{Takla} {Watershed}},
	abstract = {Before impacts of forest harvesting on Pacific salmon populations can be fully understood, the natural physical and biological influences on incubation processes must be understood within interior British Columbia watersheds. The study objective was to estimate overwinter survival of sockeye salmon (Oncorhynchus nerku) embryos within various redd micro-environments. Egg to pre-emergent fry bioassays, in conjunction with microhabitat environmental monitoring, were implemented to define a range of natural spawning conditions and their relative contribution to fry recruitment. Four adjacent tributaries (Kynoch, Forfar, Gluskie, and Bivouac creeks) of the Stuart-Takla watershed were studied during the 1993 and 1994 broodyears. Sockeye salmon successfully spawned over a wide range of habitats. Survival rates between habitat types were not significantly different in contrast to predictions generated from optimum models. This was due to the definition of marginal habitat. Spawning adults avoided truly marginal areas with intragravel dissolved oxygen levels below 3.0 mg/L. As a result, in situ redd simulations showed similar intragravel conditions in both low utilization (assumed marginal) and high utilization (assumed preferred) spawning areas. Physical (i.e., hydraulic regime, bedload characteristics) and biological (i.e., mass cleaning by high densities of spawning adults) processes result in uniformly high quality gravel conditions with permeabilities, surface water interchange, and intragravel dissolved oxygen levels associated with high incubation success. Riparian-zone substrates in the study streams were characterized by large amounts of lacustrine deposits. Reduced escapement levels, or sediment inputs which exceed current bedload transport, may impact incubation environment. By spawning early in the season (July-August), the early Stuart sockeye salmon stock enjoy advanced embryological development prior to the onset of low water temperatures. Embryos rapidly accumulate the thermal units necessary to hatch, thereby becoming- mobile in time to avoid freezing and desiccation as water-levels decline and reach seasonal minima. Embryos and alevins of the early Stuart stock can apparently tolerate temperature conditions previously considered lethal, and emerge successfully in the spring after accumulating less thermal units than any other Fraser River stock. These mechanisms, are closely linked to the stream thermal regime. Riparian forestry prescriptions must be closely monitored to detect and quantify any stream temperature changes that may impact the developmental timing of incubating salmonids.},
	author = {{R.S. Cope} and {J.S. MacDonald}},
	year = {1998},
	keywords = {2.o Sockeye Salmon, 25.a Environment / Ecosystems, 7.a Quantitative},
}

