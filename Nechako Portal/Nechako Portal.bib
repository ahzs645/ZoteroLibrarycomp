@phdthesis{shane_herbert_hartman_investigating_2013,
	address = {Prince George, BC},
	type = {Thesis ({M}.{A}) {Geography}},
	title = {Investigating the benefits and impacts of tourism development for the {Tl}'azt'en {First} {Nation} in northern {British} {Columbia}},
	abstract = {"This thesis is the result of the research that was completed with Tl’azt’en Nation. Tl’azt’en Nation partnered with the University of Northern British Columbia to form a Community University Research Alliance (CURA). This CURA project funded by the Social Science and Humanities Research Council of Canada (SSHRCC) allowed me to look at the benefits (positive) and impacts (negative) of tourism development for Tl’azt’en Nation and the issues surrounding the development of tourism in traditional Tl’azt’en territory. The research for this project was developed with the assistance of the Tl’azt’en community, and various social science techniques were employed to gather data with these community members. Techniques included: tourism presentations, workshops and semi-structured interviews. Data gathered from the community members led to the discovery of new themes and community feelings that were unexpected at the outset of the research. The research showed that Tl’azt’en Nation is in favor of tourism development, yet additional elements also emerged throughout the community members’ information sharing sessions. These alternate, unexpected elements showed that Tl’azt’en Nation community members are concerned about their culture, youth and elders. The work done in the community revealed that Tl’azt’en Nation is willing to develop tourism, but with the wish to protect and enhance their culture, youth and elders while doing so."},
	language = {English},
	school = {University of Northern British Columbia},
	author = {{Shane Herbert Hartman}},
	year = {2013},
}

@phdthesis{rurik_muenter_evaluating_2011,
	address = {Prince George, BC},
	type = {Thesis ({B}.{Sc}) {Forestry}},
	title = {Evaluating the effectiveness of public engagement in the {Prince} {George}, {McBride}, and {Burns} {Lake} community forests},
	abstract = {"Community-based resource management has the fundamental goal to involve the community in decisions. The Community Forest Agreement is a new form of forest license which the province of British Columbia is granting to community organizations in order to enable community based resource management. Community forestry is becoming an increasingly popular and widely adopted model for the province to deal with conflict laden resource issues and for communities to gain some control over the management and benefits of local forests. It is assumed that the community forests will provide control over and benefits from the local forest to the local community. There remains important questions about the effectiveness of granting forest license to community organization evaluate how effective the mechanisms for achieving local control. This thesis investigates the effectiveness of public engagement in community forestry in an attempt to understand the issues of representation, influence, and public awareness. The goal of this research project is to evaluate how the public engagement mechanisms and programs of three community forests exhibit key criteria of effective public engagement established in the literature. The research thereby validates many of these criteria and provides insight into the issues facing community forests in their efforts to involve the local public."-},
	school = {University of Northern British Columbia},
	author = {{Rurik Muenter}},
	year = {2011},
}

