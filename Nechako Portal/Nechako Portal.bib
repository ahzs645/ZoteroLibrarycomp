	keywords = {Ecosystem services, Benthic invertebrates, FISH habitats, Freshwater management, GROUND vegetation cover, Hydropower, Juncus bulbosus, MACROPHYTES, NUISANCES, PLANTS, POTAMOGETON, RIVER channels, Salmonids, VON Linne, Carl, 1707-1778},
	pages = {539--556},
}

