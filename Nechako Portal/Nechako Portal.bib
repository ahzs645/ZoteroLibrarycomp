	keywords = {cardiovascular, heart, adrenaline, catecholamine, juvenile, β-adrenoceptor (β-AR)},
	pages = {2081--2087},
	file = {Snapshot:/Users/ahmadjalil/Zotero/storage/JWS4VR2Q/abstract.html:text/html},
}

@book{gillingham_integration_2016,
