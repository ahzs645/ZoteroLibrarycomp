@article{simpson_selling_1993,
	title = {Selling the river: the {Kemano} {Completion} {Project} on {BC}'s {Nechako} {River} is a classic case of megaproject madness, where massive investments must generate massive returns, at the expense of the environment and local communities},
	volume = {3},
	copyright = {Copyright Earthkeeper Inc. Jun/Jul 1993},
	issn = {11817828},
	shorttitle = {Selling the river},
	url = {http://search.proquest.com/cbcacurrent/docview/223671662/abstract/6C1EA84A88C44F4FPQ/8?},
	abstract = {Upon completion of the tunnel, Alcan was able to divert water from the reservoir down the tunnel's 860 - metre drop to the newly completed Kemano powerhouse. The electricity from the powerhouse's eight generators, the turbines of each driven by the diverted water, was then transmitted along an 82 - kilometre, Alcan - built power line to the town of Kitimat where the company's aluminum smelter had been constructed. From the Kemano powerhouse the diverted water from the Nechako Reservoir was channelled into the Kemano River, which then carried it to the ocean. As promised, Alcan's megaproject opened up the northwest and north central portions of the province to development. Alcan was directly responsible for the creation of the town sites of Kemano (current population 250) and Kitimat (current population approximately 12,000).
While the damage the original project inflicted on the land and the people of the area was substantial, it was only the beginning. According to the 1949 agreement Alcan had the right to all of the water in the Nechako and it planned from the outset to maximize its lawful use of this resource. In 1978, Alcan announced its intention to expand its hydroelectric generating capacity by building a second tunnel through the coastal mountains to feed another generating facility at Kemano, the results of which would reduce the Nechako to 13 percent of its pre - Kenney Dam flow. Between the end of the filling of the reservoir in 1957 and 1978, Alcan had allowed the Nechako to flow at approximately 67 percent of its naturallevel. But, in 1978 the company entered into a lucrative agreement with B.C. Hydro to sell its excess power, and water flow in the river dropped dramatically.
Consequently, the 1987 Settlement Agreement was negotiated between the federal and provincial governments and Alcan, which gave Alcan the flow regimes it had wanted all along: 30 percent of the original flow until completion of the second tunnel, at which time Alcan would guarantee a minimum flow of 12 percent. In effect, Alcan gave up the rights to 12 percent of the Nechako from the original 1949 agreement.},
	language = {English},
	number = {5},
	urldate = {2015-09-04},
