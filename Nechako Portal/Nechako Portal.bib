@book{mills_hang_2005,
	title = {'{Hang} {Onto} {These} {Words}': {Johnny} {David}'s {Delgamuukw} {Evidence}},
	isbn = {978-0-8020-8534-4},
	shorttitle = {'{Hang} {Onto} {These} {Words}'},
	abstract = {In 1985 and 1986, ninety-year-old Witsuwit'en Chief, Maxlaxlex - or Johnny David as he is better known - was the first Witsuwit'en to give Commission Evidence in the Delgamuukw land claims case in which the Witsuwit'en and Gitxsan of Northern British Columbia were battling for title to their traditional territories. 'Hang Onto These Words' presents the actual transcripts of the questions and answers between lawyers working on both sides and this knowledgeable and outspoken Native elder who spoke in his own language and whose words were then translated by an interpreter into English. The evidence was given in a makeshift courtroom set up in David's own home. Anthropologist Antonia Mills was present during these proceedings, and in this book, she introduces and contextualizes the evidence within the Delgamuukw case. In his testimony, David provides a rich description of the Witsuwit'en way of life as well as the injustices suffered at the hands of Indian agents and settlers. He ends his testimony saying, "If you hang on to these words, everything will be all right." The challenge of hearing his voice, and using it to negotiate the meaning and substance of Aboriginal rights remains unresolved and resonant. In his testimony, David provides a rich description of the Witsuwit'en way of life as well as the injustices suffered at the hands of Indian agents and settlers. He ends his testimony saying, "If you hang on to these words, everything will be all right." The challenge of hearing his voice, and using it to negotiate the meaning and substance of Aboriginal rights remains unresolved and resonant.},
	language = {en},
	publisher = {University of Toronto Press},
	author = {Mills, Antonia Curtze},
	year = {2005},
	note = {Google-Books-ID: eKT8qPAcx7wC},
	keywords = {Social Science / Ethnic Studies / Native American Studies},
}

@article{mills_comparison_1988,
	title = {A {Comparison} of {Wet}'suwet'en {Cases} of the {Reincarnation} {Type} with {Gitksan} and {Beaver}},
