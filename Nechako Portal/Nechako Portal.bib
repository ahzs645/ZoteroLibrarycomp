	author = {Nguyen, P. L. and Jackson, Z. J. and Peterson, D. L.},
	month = feb,
	year = {2016},
	keywords = {Brett-type tunnel, critical swimming speed, growth, hunkering, survival, tail-beat frequency},
	pages = {655--667},
	file = {Full Text PDF:/Users/ahmadjalil/Zotero/storage/4NXTCRWH/Nguyen et al. - 2016 - Comparison of fin ray sampling methods on white st.pdf:application/pdf;Snapshot:/Users/ahmadjalil/Zotero/storage/PKJ7B5M6/abstract.html:text/html},
}

