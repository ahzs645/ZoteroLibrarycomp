@techreport{lucas_pon_assessment_2021,
	title = {Assessment of {Juvenile} {Sockeye} {Salmon} rearing in {Fraser} {Lake}, {British} {Columbia} in the fall of 2020 using paired hydroacoustic and trawl survey methods},
	abstract = {A survey of juvenile Sockeye Salmon rearing in Fraser Lake was conducted in fall of 2020 using paired hydroacoustic and trawl methods. Total Sockeye Salmon fall fry abundance (± 95\% CI) was estimated at 5,212,812 ± 2,028,465 fish. Sockeye were vertically distributed through much of the water column owing to the shallow nature of the lake, and densities were higher in the eastern third of the lake, becoming progressively lower towards the western end. All trawl-caught juvenile salmon that were genetically identified as Sockeye from the Stellako population, suggesting a low number of Kokanee in the lake relative to sympatric Sockeye Salmon. From all samples collected across the lake, the mean (± SD) fork length of juvenile Sockeye Salmon was 74.5 ± 5.9 mm, and weight was 4.68 ± 1.12 g. This was the third such survey of Fraser Lake in the last four years, but prior to 2017, the lake had not been surveyed since the late 1990’s. The renewed assessment effort in Fraser Lake follows on recommendations put forward in the Cohen Commission report, which identified the need to increase monitoring of Sockeye Salmon fall fry. With recent listing of Stellako Sockeye as a ‘Special Concern’ by COSEWIC in 2017, and the potential impacts from the slide at Big Bar on adult returns, continued monitoring and assessment of Sockeye Salmon that spawn in this system will be an important component of informed management and conservation.},
	language = {English},
	institution = {Fisheries and Oceans Canada},
	author = {{Lucas Pon} and {Garrett Lidin}},
	year = {2021},
	keywords = {2.o Sockeye Salmon, 25.a Environment / Ecosystems, 7.a Quantitative},
}

