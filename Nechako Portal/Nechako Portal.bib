@article{hill_optimizing_1997-1,
	title = {Optimizing {Aerial} {Count} {Frequency} for the {Area}-under-the-{Curve} {Method} of {Estimating} {Escapement}},
	volume = {17},
	issn = {0275-5947},
	url = {http://dx.doi.org/10.1577/1548-8675(1997)017<0461:OACFFT>2.3.CO;2},
	doi = {10.1577/1548-8675(1997)017<0461:OACFFT>2.3.CO;2},
	abstract = {The area-under-the-curve estimate of salmon escapement is computed from aerial counts of fish and estimates of the residence time of spawners. Imprecision in both of these factors leads to error in the final escapement estimate and, thus, affects its reliability as an index of escapement over time. The population of Chinook salmon Oncorhynchus tshawytscha in the Nechako River, British Columbia, was chosen for a case study, and this paper illustrates how simple simulation models can be used to explore the increases in precision about escapement estimates associated with increasing the frequency of aerial flights. Results show that confidence in escapement estimates for the Nechako River Chinook salmon population decreases as the frequency of survey flights decreases; results also show that this pattern is robust to the typically small annual differences in mean residence time in the Nechako River. Such models can help managers optimize the frequency of aerial flights by comparing the benefits of frequent flights (i.e., increased precision in escapement estimates) with the costs of those flights.},
	number = {2},
	urldate = {2016-11-23},
	journal = {North American Journal of Fisheries Management},
	author = {Hill, Ryan A.},
	month = may,
	year = {1997},
	pages = {461--466},
	file = {Full Text PDF:/Users/ahmadjalil/Zotero/storage/KN6B2TVG/Hill - 1997 - Optimizing Aerial Count Frequency for the Area-und.pdf:application/pdf;Snapshot:/Users/ahmadjalil/Zotero/storage/JGG7UJ39/1548-8675(1997)0170461OACFFT2.3.html:text/html},
}

