@misc{robertson_story_1991,
	title = {The {Story} of the {Surrender} of the {Cheslatta} {Reserves} on {April} 21, 1952.},
	shorttitle = {The {Story} of the {Surrender} of the {Cheslatta} {Reserves} on {April} 21, 1952.},
	abstract = {The people of the Cheslatta Carrier Nation lived for centuries on the shores of Cheslatta and Murray Lakes. They enjoyed a peaceful, self-sustaining existence. They hunted the moose, bear, deer and other game. They used the meat for food and the furs kept them warm in the long winters. The Cheslatta people had extensive traplines running from Uncha Lake to Tahtsa Lake and as far south as the Ulkatcho territory. The catch from these bountiful trapelines provided them with barter at areaa trading posts, as well as providing food and clothing. The land also produced a large variety of berries and plants that the people harvested each season. Medicinal plants, herbs, and roots were gathered and utilized as ancient and effective medicines. The waters of the Cheslatta, Murray, Ootsa. and many other area lakes provided the people with an endless supply of trout, char, kokanee, whitefish, and ling cod many of which were dried and smoked and stored for the winter. The Nechako River provided an important source of sockeye and Chinook salmon.},
	language = {English},
	publisher = {Cheslatta Carrier Nation Archives},
