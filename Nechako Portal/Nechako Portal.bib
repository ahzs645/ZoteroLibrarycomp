	pages = {779--794},
	file = {NRC Research Press PDF fulltext:/Users/ahmadjalil/Zotero/storage/Z4CSHZM6/Wikeem et al. - 1993 - An overview of the forage resource and beef produc.pdf:application/pdf;NRC Research Press Snapshot:/Users/ahmadjalil/Zotero/storage/G37CZ2Q7/cjas93-081.html:text/html},
