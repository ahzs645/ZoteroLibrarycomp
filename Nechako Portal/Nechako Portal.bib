	pages = {1552--1561},
	file = {NRC Research Press PDF fulltext:/Users/ahmadjalil/Zotero/storage/H5NMSHIR/Poole et al. - 2000 - Habitat use by woodland caribou near Takla Lake in.pdf:application/pdf;NRC Research Press Snapshot:/Users/ahmadjalil/Zotero/storage/VJHK5DWG/z00-087.html:text/html},
