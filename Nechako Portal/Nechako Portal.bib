	file = {Full Text PDF:/Users/ahmadjalil/Zotero/storage/XQI7VA3Q/Schiefer and Klinkenberg - 2004 - The distribution and morphometry of lakes and rese.pdf:application/pdf;Snapshot:/Users/ahmadjalil/Zotero/storage/QUT6PV57/abstract.html:text/html},
}

