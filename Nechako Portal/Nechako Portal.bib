	keywords = {FLOW, CLASSIFICATION, Complex Rock slide-Debris Avalanche, Eocene Volcanic and Volcaniclastic Rocks, Landslide, Northern British Columbia, Seismic Signal, ROCK-SLIDE, ROCKSLIDE-DEBRIS AVALANCHE},
	pages = {35--45},
}

